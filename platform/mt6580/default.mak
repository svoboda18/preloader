CFG_LOAD_AP_ROM :=0

#For CTP
CFG_LOAD_CONN_SYS :=0
#For SLT
CFG_LOAD_SLT_MD :=0
CFG_LOAD_SLT_MD32 :=0

###################################################################
# Dummy Load address
# If image header have load address, we use address in image header
# If not, we use address here
###################################################################
#For Normal Boot
CFG_UBOOT_MEMADDR       :=0x81E00000
#For Dummy AP
CFG_MD1_ROM_MEMADDR     :=0x80000000
CFG_MD1_RAMDISK_MEMADDR :=0x81400000
CFG_MD2_ROM_MEMADDR     :=0x82000000
CFG_MD2_RAMDISK_MEMADDR :=0x83400000
CFG_MD_DSP_MEMADDR      :=0x84000000
#For SLT and Dummy AP
CFG_AP_ROM_MEMADDR      :=0x86000000
#For CTP
CFG_CONN_SYS_MEMADDR    :=0x85A00000
#For SLT
CFG_TDD_MD_ROM_MEMADDR   :=0x80000000
CFG_TDD_ONLY_ROM_MEMADDR :=0x81000000
CFG_FDD_MD_ROM_MEMADDR   :=0x82000000
CFG_2G_MD_ROM_MEMADDR    :=0x83000000
CFG_MD32P_ROM_MEMADDR    :=0x84000000
CFG_MD32D_ROM_MEMADDR    :=0x85002000
CFG_BOOTA64_MEMADDR 	 :=0x80000000
CFG_DTB_MEMADDR 	 :=0x80000300
CFG_IMAGE_AARCH64_MEMADDR :=0x80080000


#For ATF
#CFG_ATF_ROM_MEMADDR   :=0x00110000
CFG_ATF_ROM_MEMADDR   :=0x00110DC0
CFG_BOOTIMG_HEADER_MEMADDR   :=0x00110800

CFG_ATF_SUPPORT :=0
